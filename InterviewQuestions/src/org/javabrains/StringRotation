package org.javabrains;

import static org.junit.Assert.assertEquals;
import static org.junit.Assert.assertTrue;

import org.junit.Test;

public class StringRotation {

	@Test
	public void test() 
	{
		String inputOne = "ChejerlaKarthik";
		String inputTwo = "KarthikChejerla";
		
		assertEquals(inputOne.length(),inputTwo.length());
			
		String concatString = inputOne.concat(inputOne);
		
		assertTrue(concatString.contains(inputTwo));
	}

}
